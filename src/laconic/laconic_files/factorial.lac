/* test if there is at least one prime number between two squre numbers*/
func zero(x) {
    x = 0;
    return;
}

func one(x) {
    x = 1;
    return;
}

func incr(x) {
    x = x + 1;
    return;
}

func decr(x) {
    x = x - 1;
    return;
}

func factorial(x) {
    if (x == 0 ) {
        x = 1;
        return;
    }
    decr(x);
    return x * factorial(x);
}

int n;

n = 2;

factorial(n);

halt;